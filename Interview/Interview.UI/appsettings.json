{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GoCWebTemplate": {
    "version": "v4_1_0", //change to current CDTS version https://cenw-wscoe.github.io/sgdc-cdts/docs/index-en.html
    "environment": "AKAMAI" //change to the appropriate environment for your application https://github.com/wet-boew/cdts-DotNetTemplates/wiki/Environments
  },

  "EmailManagerOptions": {
    "OnlySendToLoggedInUser": true
  },

  "SessionTimeoutOptions": {
    "DotNetSessionInMilliseconds": 1260000, // 21 minutes
    "CookieName": ".GoCWebTemplate.Session",
    "Enabled": true,
    "InactivityInMilliseconds": 1200000, // 20 minutes
    "ReactionTimeInMilliseconds": 180000, // 3 minutes for
    "SessionAliveInMilliseconds": 1200000, // 20 minutes
    "RefreshLimitInMilliseconds": 60000, // 1 minute
    "RefreshOnClick": true,
    "Method": "GET"
  }

}
