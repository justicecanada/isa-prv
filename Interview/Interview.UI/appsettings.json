{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GoCWebTemplate": {
    "version": "v4_1_0", //change to current CDTS version https://cenw-wscoe.github.io/sgdc-cdts/docs/index-en.html
    "environment": "AKAMAI" //change to the appropriate environment for your application https://github.com/wet-boew/cdts-DotNetTemplates/wiki/Environments
  },

  "EmailManagerOptions": {
    "OnlySendToLoggedInUser": true
  },

  "SessionTimeoutOptions": {
    "DotNetSessionInMilliseconds": 360000, // 6 minutes for testing
    "CookieName": ".GoCWebTemplate.Session",
    "Enabled": true,
    "InactivityInMilliseconds": 180000, // 3 minutes for testing
    "ReactionTimeInMilliseconds": 120000, // 2 minutes for testing
    "SessionAliveInMilliseconds": 180000, // 3 minutes for testing
    "RefreshLimitInMilliseconds": 60000, // 1 minute for testing
    "RefreshOnClick": false,
    "Method": "GET"
  }

}
