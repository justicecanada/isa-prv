
@{

    string label = ViewData["InterviewTileLabel"].ToString();
    string number = ViewData["InterviewTileNumber"].ToString();

}

<div class="well well-sm text-center">
    <div class="row">
        <div class="col-md-12">
            <span class="h3">@label</span>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <span class="h3">@number</span>
        </div>
    </div>
</div>